# Trust Service Configuration

# Server Configuration
PORT=3002                                    # Port on which the Trust Service will listen

# Kafka Configuration
KAFKA_BROKER=localhost:9092                  # Kafka broker address for consuming evaluation job messages
KAFKA_GROUP_ID=trust-service-group           # Kafka consumer group ID for the Trust Service

# Database Configuration
DB_HOST=localhost                            # PostgreSQL database host
DB_PORT=5432                                 # PostgreSQL database port
DB_NAME=ai_evaluation_platform               # PostgreSQL database name
DB_USER=postgres                             # PostgreSQL database user
DB_PASSWORD=postgres                         # PostgreSQL database password

# Ground Truth Data Configuration
GROUND_TRUTH_CSV_PATH=./data/ground_truth.csv # Path to the CSV file containing ground truth data for evaluations
